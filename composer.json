{
    "name": "elegondigital/framework",
    "description": "Set of packages to facilitate new projects at Elegon",
    "version": "master",
    "authors": [
        {
            "name": "Loris Leiva",
            "email": "loris.leiva@gmail.com"
        },
        {
            "name": "Simon Favre",
            "email": "sim.favre73@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Elegon\\": "src/"
        },
        "files": [
            "src/Foundation/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Elegon\\Tests\\": "tests/"
        }
    },
    "dist": {
        "type": "git",
        "url": "git@bitbucket.org:elegondigital/framework.git"
    }
}